{
  "dashboard": {
    "title": "MySQL Database Dashboard",
    "tags": ["mysql", "database"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "MySQL Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "mysql_global_status_threads_connected",
            "legendFormat": "Connected Threads",
            "refId": "A"
          },
          {
            "expr": "mysql_global_variables_max_connections",
            "legendFormat": "Max Connections",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "MySQL Queries",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(mysql_global_status_queries[5m])",
            "legendFormat": "Queries/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "MySQL Slow Queries",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(mysql_global_status_slow_queries[5m])",
            "legendFormat": "Slow Queries/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "MySQL Table Locks",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(mysql_global_status_table_locks_waited[5m])",
            "legendFormat": "Table Locks Waited/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "MySQL InnoDB Buffer Pool",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "mysql_global_status_innodb_buffer_pool_read_requests",
            "legendFormat": "Buffer Pool Read Requests",
            "refId": "A"
          },
          {
            "expr": "mysql_global_status_innodb_buffer_pool_reads",
            "legendFormat": "Buffer Pool Reads",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "MySQL Network Traffic",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(mysql_global_status_bytes_received[5m])",
            "legendFormat": "Bytes Received/sec",
            "refId": "A"
          },
          {
            "expr": "rate(mysql_global_status_bytes_sent[5m])",
            "legendFormat": "Bytes Sent/sec",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "Bps"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}

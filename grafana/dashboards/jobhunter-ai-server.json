  "panels": [
    {
      "type": "timeseries",
      "title": "CPU Usage (%)",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green" }, { "color": "yellow", "value": 70 }, { "color": "red", "value": 90 }] },
          "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        { "refId": "A", "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Memory Usage",
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "bytes", "custom": { "fillOpacity": 10 } }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "refId": "A", "expr": "jobhunter_ai_process_resident_memory_bytes", "legendFormat": "Process Memory" },
        { "refId": "B", "expr": "node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes", "legendFormat": "System Memory Used" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Network Traffic",
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "Bps", "custom": { "fillOpacity": 10 } }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "refId": "A", "expr": "rate(node_network_receive_bytes_total{device!~\"lo|docker.*\"}[5m])", "legendFormat": "Receive - {{device}}" },
        { "refId": "B", "expr": "rate(node_network_transmit_bytes_total{device!~\"lo|docker.*\"}[5m])", "legendFormat": "Transmit - {{device}}" }
      ]
    },
    {

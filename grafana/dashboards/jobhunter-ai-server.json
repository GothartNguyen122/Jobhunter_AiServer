      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "fillOpacity": 10 } }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom", "calcs": ["sum", "mean"] }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "refId": "A", "expr": "sum(rate(jobhunter_ai_http_requests_total[5m])) by (method, status)", "legendFormat": "{{method}} - {{status}}" }
      ]
    },
    {
